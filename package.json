    "build": "better-npm-run build",
    "dev": "concurrently \"better-npm-run dev\" \"better-npm-run serve\"",
    "lint": "eslint --ext .ts --ext .tsx .",
    "prebuild": "rimraf node_modules/.cache",
    "predev": "rimraf node_modules/.cache",
    "serve": "better-npm-run serve"
  },
  "betterScripts": {
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "WEBPACK_CONFIG": [
          "webpack.config.prod.ts"
        ]
      }
    },
    "dev": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "development",
        "WEBPACK_CONFIG": [
          "webpack.config.dev.ts"
        ]
      }
    },
    "serve": {
      "command": "serve --debug --listen $PORT ./dist",
      "env": {}
    }
  },
